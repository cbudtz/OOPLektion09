<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.Scene?>
<?import javafx.scene.shape.Rectangle?>
<Scene xmlns="http://javafx.com/javafx"
       xmlns:fx="http://javafx.com/fxml"
       fx:controller="control.MainGuiController"
       width="800" height="600" onKeyPressed="#handleKeyPress">
<AnchorPane prefWidth="800" prefHeight="600">
    <Label>This is main stuff!</Label>
    <Rectangle fx:id="rect" width="10" height="200" x="10" y="10"/>
    <TableView fx:id="tableview"/>
</AnchorPane>
</Scene>
